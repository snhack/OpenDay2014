            <ITEXT FONTSIZE="13" CH="We'll need two of them, we can just mirror this part across the other side using &quot;mirror&quot;:"/>
            <para/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    ..."/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    "/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    // arms (1)"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    mirror([]) rotate(a=30, v=[0,1,0]) translate([0, 0, -4]) {"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="        cylinder(h = 4, r = 1);"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="        sphere(r=1);"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    }"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    // arms (2)"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    rotate(a=30, v=[0,1,0]) translate([0, 0, -4]) {"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="        cylinder(h = 4, r = 1);"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="        sphere(r=1);"/>
            <para/>
            <ITEXT FONT="FreeMono Regular" FONTSIZE="13" CH="    }"/>
            <para/>
            <para/>
            <ITEXT FONTSIZE="13" CH="Note how we wrapped the arm+hand (cylinder+sphere) in curly braces to"/>
